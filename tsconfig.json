{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "declaration": true,
    "declarationDir": "dist/types",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "lib": [
      "es6",
      "dom",
      "dom.iterable",
      "scripthost",
      "es2016.Array.Include",
    ],
    "module": "es6",
    "moduleResolution": "node",
    "newLine": "lf",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "paths": {
      "@/*": [
        "src/*",
      ],
    },
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": true,
    "target": "es6",
    "types": [
      "node",
      "vue",
      "vite/client",
      "vitest/globals",
      "@intlify/unplugin-vue-i18n/messages"
    ],
  },
  "exclude": [
    "src/**/__tests__/*",
    "src/**/*.cy.ts",
    "**/*.spec.ts",
    "/dist",
    "node_modules",
  ],
  "include": [],
  "references": [],
}
